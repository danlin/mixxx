<!DOCTYPE skin>
<skin>
    <manifest>
        <title>QTQuick 2 Demo</title>
        <author>Daniel Lindenfelser</author>
        <version>1.0.0</version>
        <description>QTQuick 2 (QT 5.x) Demo Skin</description>
        <language>en</language>
        <license>Creative Commons Attribution, Share-Alike 3.0 Unported</license>
        <attributes>
            <attribute config_key="[Master],num_decks">2</attribute>
            <attribute config_key="[Master],num_samplers">4</attribute>
            <attribute config_key="[Master],num_preview_decks">1</attribute>
        </attributes>
	</manifest>

    <MinimumSize>800,600</MinimumSize>
    <SizePolicy>me,me</SizePolicy>
    <Layout>vertical</Layout>
    <Children>
        <QtQuick>
            <MinimumSize>800,600</MinimumSize>
            <Size>me,me</Size>
            <Path>Application/Main.qml</Path>
        </QtQuick>
    </Children>
</skin>
